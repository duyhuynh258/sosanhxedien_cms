{
  "kind": "collectionType",
  "collectionName": "ev_models",
  "info": {
    "singularName": "ev-model",
    "pluralName": "ev-models",
    "displayName": "EVModel",
    "description": "Electric Vehicle Models"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "model_name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ev-brand.ev-brand",
      "inversedBy": "models"
    },
    "year": {
      "type": "string",
      "required": true
    },
    "price_vnd": {
      "type": "biginteger",
      "required": true
    },
    "range_km": {
      "type": "integer",
      "required": true
    },
    "battery_kwh": {
      "type": "decimal",
      "required": true
    },
    "charge_time": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "model_name",
      "required": true
    },
    "acceleration": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "top_speed": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "power": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "torque": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "drive_type": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seating_capacity": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "dimensions": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "wheelbase": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ground_clearance": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "weight": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cargo_volume": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "warranty": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "review_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::review-article.review-article",
      "mappedBy": "ev_models"
    }
  }
}
